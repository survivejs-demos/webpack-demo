{
  "plugins": [
    "syntax-dynamic-import"
  ],
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["chrome >= 60"]
        }
      }
    ],
    "react"
  ],
  "env": {
    "karma": {
      "plugins": [
        [
          "istanbul",
          { "exclude": ["tests/*.test.js"] }
        ]
      ]
    }
  }
}
